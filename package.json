{
  "name": "xrtalk-web",
  "version": "0.0.1",
  "description": "XR Talk Web",
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --host 0.0.0.0 --port 9000 --config webpack.dev.config.js --inline --open --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.dev.config.js --mode development",
    "build.prod": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.prod.config.js -p",
    "build.esnext": "yarn run build:types && yarn run build:js",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "yarn run build.prod && yarn run build.esnext"
  },
  "keywords": [],
  "author": "Paul Reynolds",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "source-map": "^0.7.3",
    "source-map-loader": "0.2.3",
    "ts-loader": "^6.2.1",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.8.2",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@babylonjs/core": "^4.1.0",
    "@babylonjs/gui": "^4.1.0",
    "@babylonjs/loaders": "^4.1.0",
    "@babylonjs/materials": "^4.1.0",
    "@babylonjs/post-processes": "^4.1.0",
    "@babylonjs/procedural-textures": "^4.1.0",
    "@babylonjs/serializers": "^4.1.0",
    "@magicleap/prismatic": "^2.0.4",
    "@types/node": "^13.7.7",
    "pepjs": "^0.5.2",
    "webxr-polyfill": "^2.0.1"
  }
}
